{
    "plugins": [
        "prettier-plugin-svelte"
    ],
    "pluginSearchDirs": [
        "."
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ],
    "useTabs": false,
    "tabWidth": 2,
    "printWidth": 90,
    "singleQuote": true,
    "semi": true
}